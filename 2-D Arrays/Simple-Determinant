import java.io.*; 
import java.util.*; 


class Main {
	public static void main (String[] args) {
              Scanner s= new Scanner(System.in);
	          int a= s.nextInt();
	          int b= s.nextInt();
			  int c= s.nextInt();
			  int d= s.nextInt();
			  System.out.println((a*d)-(c*b));
	}
}

/*Sample Input
4 5
2 3

Sample Output
