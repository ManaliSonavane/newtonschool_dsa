import java.util.*;
import java.io.*;

public class Main
{
	public static void main(String[] args) {
	  String s1="Manali";
		System.out.println(s1);
    
		Scanner s= new Scanner(System.in);
		String s2=s.next();
		System.out.println(s2);
    
    //To access more than one word use nextline
		String s3=s.nextLine();
		System.out.println(s3);
		System.out.println();
    
		//Concatenating
		String fn="kim";
		String ln="seokjin";
		//Spaces are valid characters in string
		String fulln=fn+" "+ln;
		System.out.println(fulln);
		System.out.println(fulln.length());
		System.out.println();
    
		//Empty strings
		String d="";
		System.out.println(d);
		
		//chartAt
		for(int i=0;i<fulln.length();i++){
		    System.out.println(fulln.charAt(i));
		}
		
    
		//compare 2 strings
		String n1="manali";
		String n2="Manalinali";
		//1 n1 > n2   +ve value
		//2 n1=n2      0
		//3 n1 < n2    -ve value
		if(n1.compareTo(n2)==0){
		    System.out.println("equal");    
		}
		else{
		    System.out.println("not equal");
		}
		System.out.println();
		if(n1==n2){
		    System.out.println("equal");    
		}
		else{
		    System.out.println("not equal");
		}
	}
}
