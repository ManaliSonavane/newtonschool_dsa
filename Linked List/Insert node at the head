/*
class Node {
    Node next;
    int val;

    Node(int val) {
        this.val = val;
        next = null;
    }
}
*/

public static Node addElement(Node head, int k) {
             //No need of of checking if head==null; 
             Node newNode= new Node(5);
             newNode.val =k;
             newNode.next=head;
             head = newNode;
             return head;
}


/* 
Sample Input:-
5 2
1 2 3 4 5 

Sample Output:
2 1 2 3 4 5
*/
