import java.util.*;
import java.io.*;

public class Main
{
	public static void main(String[] args) {
		Scanner s=new Scanner(System.in);
		int n= s.nextInt();
		int[] arr= new int[n];
		//n=5 if i=0; (0,1,2,3,4)
		for (int i=0;i<n;i++){
		    arr[i]= s.nextInt();
		}
		int sum=0;
		for (int i=0;i<n;i++){
		   sum= sum+ arr[i];
		}
		System.out.println(sum);
	}
}
